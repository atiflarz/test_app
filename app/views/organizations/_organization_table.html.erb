<table class="table  table-striped table-bordered table-hover", id="organization_table">
  <thead>
    <tr>
      <th>ID</th>
      <th>ORG NAME</th>
      <th>ORG IDENTIFIER</th>
      <th>CREATED BY</th>
      <th>COUNTRY</th>
      <th>CREATED AT</th>
      <th>UPDATED AT</th>
    </tr>
  </thead>
  <tbody>
      <% @organizations.each do |organization| %>
        <tr class="odd gradeX">
        <td><%= organization.id%></td>
        <td><%= organization.org_name%></td>
        <td><%= organization.org_identifier%></td>
        <td><%= organization.user.email%></td>
        <td><%=organization.address.country%></td>
        <td><%=organization.created_at.strftime("%m/%d/%Y")%></td>
        <td><%=organization.updated_at.strftime("%m/%d/%Y")%></td>
        </tr>
      <% end %>
  </tbody>
</table>
